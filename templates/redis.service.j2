[Unit]
Description=Redis persistent key-value database - port {{ item }}
After=network.target

[Service]
ExecStart=/usr/bin/redis-server /etc/redis_{{ item }}.conf --daemonize no
ExecStop=/usr/bin/redis-shutdown redis_{{ item }}
User=redis
Group=redis

[Install]
WantedBy=multi-user.target
